/ {
    chosen {
		zephyr,console = &usart2;
	};

    leds {
        compatible = "gpio-leds";
        led: led {
            gpios = <&gpioc 13 GPIO_ACTIVE_HIGH>;
            label = "LD";
        };
    };

    aliases {
        led0 = &led;
    };
};

&usart1 {
    status = "disabled";
};

/* Enable PWM */
&timers1 {
    status = "okay";
    st,prescaler = <4>;

    pwm1: pwm {
        status = "okay";
        pinctrl-0 = <&tim1_ch1_pwm_out_pa8 &tim1_ch2_pwm_out_pa9 
                        &tim1_ch3_pwm_out_pa10 &tim1_ch4_pwm_out_pa11>;
        pinctrl-names = "default";
    };

};

&spi1 {
    cs-gpios = <&gpioa 4 GPIO_ACTIVE_LOW>;

	sdhc0: sdhc@0 {
                compatible = "zephyr,sdhc-spi-slot";
                reg = <0>;
                status = "okay";
                mmc {
                    compatible = "zephyr,sdmmc-disk";
                    disk-name = "SD";
                    status = "okay";
                };
                spi-max-frequency = <24000000>;
        };
};

&i2c1 {
    main_imu: mpu6050@68 {
        compatible = "invensense,mpu6050";
        reg = <0x68>;
        status = "okay";
        int-gpios = <&gpiob 5 GPIO_ACTIVE_HIGH>;
    };
};








